{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "vueIndentScriptAndStyle": false,
    "tabWidth": 4,
    "endOfLine": "auto",
    "overrides": [
      {
        "files": ["*.scss"],
        "options": {
          "trailingComma": "all",
          "tabWidth": 2
        }
      }
    ]
  }
  